// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/out": true
    },
    "typescript.tsdk": "./node_modules/typescript/lib",

    "[markdown]": {
        "editor.rulers": [
            85
        ],
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 85
    }
}